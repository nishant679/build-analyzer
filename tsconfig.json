{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es2020",
	  "lib": [
		"es2020",
		"dom"
	  ],
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "jsx": "react-jsx",
	  "outDir": "out"
	},
	"exclude": [
	  "node_modules",
	  ".vscode-test",
	  "src/webview/app/node_modules",
	  "src/webview/app/dist"
	]
  }
  